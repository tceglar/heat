  "layers": [
    {
      "id": "custom-lines-layer",
      "type": "line",
      "source": "some-lines-source",
      "layout": {
        "line-join": "round",
        "line-cap": "round"
      },
      "paint": {
        "line-color": "#0000ff",
        "line-width": 2.5,
        "line-dasharray": [1, 2]
      },
      "filter": ["in", "$type", "LineString"]
    },
    {
      "id": "custom-points-layer",
      "type": "circle",
      "source": "some-points-source",
      "paint": {
        "circle-radius": 4,
        "circle-color": "#ff0000",
        "circle-stroke-width": 2,
        "circle-stroke-color": "#fff"
      },
      "filter": ["in", "$type", "Point"]
    }
  ]
